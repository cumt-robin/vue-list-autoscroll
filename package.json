{
  "name": "vue-autoscroll",
  "version": "1.0.0",
  "description": "a Vue horizontal scroll directive, and you can pass parameters to control the details",
  "main": "dist/vue-autoscroll.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config build/webpack.prod.conf.js",
    "postbuild": "cross-env NODE_ENV=production webpack --progress --config build/webpack.minify.conf.js",
    "build:demo": "cross-env NODE_ENV=production IS_DEMO=true webpack --progress --config build/webpack.demo.conf.js",
    "analyze:build": "cross-env report=true npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "keywords": [
    "vue-autoscroll",
    "autoscroll",
    "vue",
    "directive"
  ],
  "author": "tusi666",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cumt-robin/vue-autoscroll.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "core-js": "^3.6.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-component": "^1.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.5.1",
    "eslint": "^6.8.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "vue-eslint-parser": "^7.0.0",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "sideEffects": [
    "*.scss",
    "*.css",
    "*.vue"
  ]
}
